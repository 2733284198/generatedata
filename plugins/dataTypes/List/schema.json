{
	"title": "List",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"listType": {
			"enum": ["exactly", "atMost"]
		},
		"exactly": {
			"type": "number"
		},
		"atMost": {
			"type": "number"
		},
		"option": {
			"type": "string"
		}
	},
	"required": ["listType", "option"]
}
